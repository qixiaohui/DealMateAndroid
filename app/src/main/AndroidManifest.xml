<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dealmate.xiaohui.dealmate">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.DealListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DealDetailAjaxActivity">
        </activity>
        <activity android:name=".activity.DealWebViewActivity">
        </activity>
        <activity android:name=".activity.ListFavoriteActivity">
        </activity>
        <activity android:name=".activity.DealDetailSqlActivity">
        </activity>
    </application>

</manifest>